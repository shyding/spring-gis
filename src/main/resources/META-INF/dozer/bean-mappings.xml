<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <stop-on-errors>true</stop-on-errors>
        <date-format>MM/dd/yyyy HH:mm</date-format>
        <wildcard>true</wildcard>
    </configuration>

    <mapping>
        <class-a>io.interface21.examples.gis.integration.jpa.User</class-a>
        <class-b>io.interface21.examples.gis.UserVO</class-b>
        <field>
            <a is-accessible="true">username</a>
            <b is-accessible="true">username</b>
        </field>
        <field>
            <a is-accessible="true">home</a>
            <b is-accessible="true">home</b>
        </field>
    </mapping>

</mappings>